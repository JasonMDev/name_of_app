<%= form_tag("/products", method: "get", role: "search", class:"navbar-form navbar-left" ) do %>
  <div class="form-group">        
    <%= text_field_tag :q, nil, class: "form-control", placeholder: "Search Product" %>
  </div>
  <%= submit_tag("Go", class: "btn btn-default form-search", id: "btn-search") %>
<% end %>
